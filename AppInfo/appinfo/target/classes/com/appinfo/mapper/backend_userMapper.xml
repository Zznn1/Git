<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.appinfo.mapper.backend_userMapper" >
  <resultMap id="BaseResultMap" type="com.appinfo.entity.backend_user" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="userCode" property="usercode" jdbcType="VARCHAR" />
    <result column="userName" property="username" jdbcType="VARCHAR" />
    <result column="userType" property="usertype" jdbcType="BIGINT" />
    <result column="createdBy" property="createdby" jdbcType="BIGINT" />
    <result column="creationDate" property="creationdate" jdbcType="TIMESTAMP" />
    <result column="modifyBy" property="modifyby" jdbcType="BIGINT" />
    <result column="modifyDate" property="modifydate" jdbcType="TIMESTAMP" />
    <result column="userPassword" property="userpassword" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, userCode, userName, userType, createdBy, creationDate, modifyBy, modifyDate, 
    userPassword
  </sql>
  <select id="login" resultType="com.appinfo.entity.backend_user">
        select * from backend_user where usercode=#{usercode} and userpassword=#{userpassword}
    </select>
</mapper>